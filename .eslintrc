{
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true,
      "arrowFunctions": true,
      "classes": true,
      "modules": true,
      "defaultParams": true
    },
    "sourceType": "module",
    "ecmaVersion": 7
  },
  "parser": "babel-eslint",
  "rules": {
    "quotes": [2, "single"],
    "no-console": 0,
    "no-debugger": 2,
    "semi": 0,
    "no-control-regex": 2,
    "linebreak-style": [0, "error", "windows"],
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "array-bracket-spacing": [2, "never"],
    "no-irregular-whitespace": 0,
    "no-trailing-spaces": 2,
    "eol-last": 0,
    "no-unused-vars": [2, {"vars": "all", "args": "after-used"}],
    "no-underscore-dangle": 0,
    "no-alert": 2,
    "no-lone-blocks": 0,
    "no-class-assign": 2,
    "no-floating-decimal": 2,
    "no-loop-func":1,
    "no-cond-assign": 2,
    "no-delete-var": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-dupe-args": 2,
    "no-empty": 2,
    "no-func-assign": 2,
    "no-invalid-this": 0,
    "no-redeclare": 2,
    "no-spaced-func": 2,
    "no-use-before-define": 2,
    "camelcase": 0,
    "no-unreachable": 1,
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "comma-spacing": [2, {"before": false, "after": true}],
    "no-mixed-spaces-and-tabs": 0,
    "prefer-arrow-callback": 0,
    "arrow-parens": 2,
    "arrow-spacing":[2,{ "before": true, "after": true }],
    "constructor-super": 0,
    "no-const-assign":2,
    "no-dupe-class-members":2,
    "no-this-before-super": 2,
    "no-var": 0,
    "object-shorthand": 0,
    "prefer-template":0,
    "no-case-declarations": 0
  },
  "settings": {
    "import/ignore": [
      "node_modules"
    ]
  }  
}